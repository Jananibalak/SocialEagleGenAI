      "parameters": {
        "jsCode": "// Extract artist/song from message\nconst message = $('Telegram Trigger').first().json.message.text.toLowerCase().trim();\nlet song=\"\";\nlet extractedText = '';\n\n// Parse different command formats\nif (message.includes('play ')) {\n  extractedText = message.split('play ')[1];\n} else if (message.includes('music ')) {\n  extractedText = message.split('music ')[1];\n}\n\nif (extractedText) {\n  song = extractedText.trim();\n}\nelse\n{\n  song=\"Whistle podu\";\n}\n\n\n\nreturn {\n  json: {\n    song: song,\n    original_message: message,\n    user_name:$('Telegram Trigger').first().json.message.from.first_name,\n    command_type: 'music_play'\n  }\n};"
      },
